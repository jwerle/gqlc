{
  "name": "gqlc",
  "version": "0.0.1",
  "description": "GraphQL Schema Compiler",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "examples": "npm run examples/basic && npm run examples/advanced",
    "examples/basic": "node -e 'require(\"babel-core/register\"); require(\"./examples/basic/main\")'",
    "examples/advanced": "node -e 'require(\"babel-core/register\"); require(\"./examples/advanced/main\")'"
  },
  "keywords": [
    "graphql",
    "gql",
    "compiler",
    "schema"
  ],
  "author": "Joseph Werle <joseph.werle@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.4.0",
    "extend": "^3.0.0",
    "graphql": "^0.4.14",
    "invariant": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "esdoc": "^0.4.3",
    "mocha": "^2.3.4"
  }
}
